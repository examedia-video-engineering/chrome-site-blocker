{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "permissions": ["storage", "declarativeNetRequest"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  }
}